{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "fui": {
      "projectType": "library",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/fui",
      "sourceRoot": "projects/fui/src",
      "prefix": "fui",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/fui/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/fui/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/fui/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/fui/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "fui-demo": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/fui-demo",
      "sourceRoot": "projects/fui-demo/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "allowedCommonJsDependencies": [
              "dayjs",
              "dayjs/plugin/customParseFormat"
            ],
            "outputPath": "dist/fui-demo",
            "index": "projects/fui-demo/src/index.html",
            "browser": "projects/fui-demo/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/fui-demo/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/fui-demo/src/favicon.ico",
              "projects/fui-demo/src/assets"
            ],
            "styles": [
              "projects/fui-demo/src/styles.scss"
            ],
            "scripts": [],
            "server": "projects/fui-demo/src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "projects/fui-demo/server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "100mb",
                  "maximumError": "100mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "100mb",
                  "maximumError": "100mb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "fui-demo:build:production"
            },
            "development": {
              "buildTarget": "fui-demo:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "fui-demo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/fui-demo/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/fui-demo/src/favicon.ico",
              "projects/fui-demo/src/assets"
            ],
            "styles": [
              "projects/fui-demo/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "fui-v2": {
      "projectType": "library",
      "root": "projects/fui-v2",
      "sourceRoot": "projects/fui-v2/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/fui-v2/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/fui-v2/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/fui-v2/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/fui-v2/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "fui-demo-v2": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/fui-demo-v2",
      "sourceRoot": "projects/fui-demo-v2/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/fui-demo-v2",
            "index": "projects/fui-demo-v2/src/index.html",
            "browser": "projects/fui-demo-v2/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/fui-demo-v2/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/fui-demo-v2/src/favicon.ico",
              "projects/fui-demo-v2/src/assets"
            ],
            "styles": [
              "projects/fui-demo-v2/src/styles.scss"
            ],
            "scripts": [],
            "server": "projects/fui-demo-v2/src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "projects/fui-demo-v2/server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "fui-demo-v2:build:production"
            },
            "development": {
              "buildTarget": "fui-demo-v2:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "fui-demo-v2:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/fui-demo-v2/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/fui-demo-v2/src/favicon.ico",
              "projects/fui-demo-v2/src/assets"
            ],
            "styles": [
              "projects/fui-demo-v2/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
