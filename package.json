{
  "name": "fundamentum-ui",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:demo": "node dist/demo/server/server.mjs",
    "compodoc:build": "compodoc -p tsconfig.doc.json -d documentation -c compodoc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s",
    "fui-start": "node cli-interface && npm run fui-build && npm run fui-serve",
    "fui-build": "concurrently \"compodoc -p tsconfig.doc.json -d documentation -c compodoc.json\" \"ng build fui\"",
    "fui-serve": "concurrently \"ng serve\" \"npm run compodoc:serve\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.0",
    "@angular/common": "^17.1.0",
    "@angular/compiler": "^17.1.0",
    "@angular/core": "^17.1.0",
    "@angular/forms": "^17.1.0",
    "@angular/platform-browser": "^17.1.0",
    "@angular/platform-browser-dynamic": "^17.1.0",
    "@angular/platform-server": "^17.1.0",
    "@angular/router": "^17.1.0",
    "@angular/ssr": "^17.1.0",
    "echarts": "^5.4.3",
    "express": "^4.18.2",
    "highlight.js": "^11.9.0",
    "ngx-highlightjs": "^11.0.0-beta.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "vite": "^5.1.1",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.0",
    "@angular/cli": "^17.1.0",
    "@angular/compiler-cli": "^17.1.0",
    "@compodoc/compodoc": "^1.1.23",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.19.33",
    "concurrently": "^8.2.2",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.1.0",
    "typescript": "~5.3.2"
  }
}
